<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="MyTitleBar">
        <attr name="titleText" format="string|reference" />
        <attr name="titleSize" format="integer" />
        <attr name="titleColor" format="reference" />

        <attr name="logoSrc" format="reference" />

        <attr name="leftImgId" format="reference" />
        <attr name="leftTextColor" format="reference" />
        <attr name="leftText" format="string" />

        <attr name="rightImgId" format="reference" />
        <attr name="rightText" format="string" />
        <attr name="rightTextColor" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomPasswordInputView">
        <attr name="maxCount" format="integer" />
        <attr name="pwdcircleColor" format="color" />
        <attr name="pwdCircleRadius" format="dimension" />
        <attr name="strokeColor" format="color" />
        <attr name="devideLineColor" format="color" />
        <attr name="divideLineWidth" format="dimension" />
        <attr name="rectAngle" format="dimension" />

    </declare-styleable>
    <declare-styleable name="PayPwdView">
        <attr name="border_color" format="color" />
        <attr name="dot_color" format="dimension" />
        <attr name="count" format="integer" />
    </declare-styleable>

</resources>